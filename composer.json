{
    "name": "stefanotorresi/my-backend",
    "description": "A Zend Framework 2 module that provides a basic and extensible admin backend UI through ZfcUser and ZfcRbac",
    "type": "library",
    "keywords": [
        "zf2",
        "zend",
        "module",
        "user",
        "admin",
        "backend"
    ],
    "homepage": "https://github.com/stefanotorresi/MyBackend",
    "authors": [
        {
            "name": "Stefano Torresi",
            "email": "webdev@stefanotorresi.it",
            "homepage": "http://stefanotorresi.it"
        }
    ],
    "require": {
        "zf-commons/zfc-user-doctrine-orm": "*",
        "zf-commons/zfc-rbac": "*"
    },
    "require-dev": {
        "doctrine/doctrine-module": "0.8.*@dev",
        "doctrine/doctrine-orm-module": "0.8.*@dev",
        "doctrine/data-fixtures": "dev-master",
        "hounddog/doctrine-data-fixture-module": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "MyBackend": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "license": "MIT"
}
